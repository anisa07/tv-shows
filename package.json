    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.7",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^16.0.0",
    "prettier": "3.5.2",
    "typescript": "~5.7.2",
